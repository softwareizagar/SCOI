	<queryString language="SQL">
		<![CDATA[select * from ( select tb_empleado.folio as Folio
						,tb_empleado.nombre + ' ' +	tb_empleado.ap_paterno + ' ' +  tb_empleado.ap_materno as NombreCompleto
						,tb_establecimiento.nombre as Establecimiento
						,CASE 
							WHEN tb_empleado.status = 1
								THEN 'VIGENTE' 
							WHEN tb_empleado.status = 2
								THEN 'VACASIONES'
							WHEN tb_empleado.status = 3
								THEN 'BAJA'
							WHEN tb_empleado.status = 4
								THEN 'NO CONTRATABLE'
						END as status
						,abs(isnull((select ROUND(SUM(case when (status_de_cobro_seguridad = 2 and diferencia_seguridad_sugerido <> 0) then diferencia_seguridad_sugerido
																when (tb_alimentacion_de_cortes.status_de_cobro_seguridad = 2 and tb_alimentacion_de_cortes.diferencia_seguridad_sugerido = 0 and tb_alimentacion_de_cortes.diferencia_auditoria <> 0) then tb_alimentacion_de_cortes.diferencia_auditoria
																when (status_de_cobro_auditoria = 2 and diferencia_auditoria <> 0) then diferencia_auditoria
																else diferiencia_de_corte
															end),2)
													 from tb_alimentacion_de_cortes
													where folio_empleado = tb_empleado.folio
													and status_autorizacion_corte = 1
													and status = 1),0)) as adeudo
						,tb_abonos.a_favor as saldo_a_favor
						,tb_abonos.abono
						,(abs(isnull((select ROUND(SUM(case when (status_de_cobro_seguridad = 2 and diferencia_seguridad_sugerido <> 0) then diferencia_seguridad_sugerido
															when (tb_alimentacion_de_cortes.status_de_cobro_seguridad = 2 and tb_alimentacion_de_cortes.diferencia_seguridad_sugerido = 0 and tb_alimentacion_de_cortes.diferencia_auditoria <> 0) then tb_alimentacion_de_cortes.diferencia_auditoria
															when (status_de_cobro_auditoria = 2 and diferencia_auditoria <> 0) then diferencia_auditoria
															else diferiencia_de_corte
														end),2)
												 from tb_alimentacion_de_cortes
												where folio_empleado = tb_empleado.folio
												and status_autorizacion_corte = 1
												and status = 1),0)) -tb_abonos.a_favor-tb_abonos.abono) as diferencia_total
				from tb_empleado
				inner join tb_establecimiento on tb_establecimiento.folio = tb_empleado.establecimiento_id
				inner join tb_abonos on tb_abonos.folio_empleado = tb_empleado.folio
				where tb_empleado.status = '1'
				and tb_empleado.puesto_id in (32,52) ) a
where a.adeudo <> 0
order by Establecimiento asc]]>
	</queryString>
